<?xml version="1.0" encoding="utf-8"?>
<!-- Configurações de backup seguro para Eletricarlos -->
<full-backup-content>
    <!-- Incluir SharedPreferences com dados do usuário -->
    <include domain="sharedpref" path="eletricarlos_data.xml"/>
    <include domain="sharedpref" path="eletricarlos_auth.xml"/>
    
    <!-- Excluir dados sensíveis temporários -->
    <exclude domain="file" path="temp/"/>
    <exclude domain="cache" path="."/>
    <exclude domain="external" path="."/>
</full-backup-content>